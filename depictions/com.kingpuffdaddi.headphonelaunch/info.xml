<package>
	<id>com.kingpuffdaddi.headphonelaunch</id>
	<name>HeadphoneLaunch</name>
	<version>0.0.2</version>
	<compatibility>
		<firmware>
			<miniOS>8.0</miniOS>
			<maxiOS>12.4</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies></dependencies>
	<descriptionlist>
		<description> Automatically launch/kill an app when you connect/disconnect headphones. Configure in settings</description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<image>cydia.png</image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Start app from LockScreen and trigger passcode view</change>
		<change>Remove app from AppSwitcher when it gets killed</change>
	</changelog>
	<links></links>
</package>