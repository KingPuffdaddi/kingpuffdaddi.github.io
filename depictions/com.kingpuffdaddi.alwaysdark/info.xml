	<screenshots>
		<screenshot>
			<image>cydia.png</image>
		</screenshot>
		<screenshot>
			<image>settings.png</image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Changed the 3D touch icons</change>
	</changelog>
	<links></links>
</package>