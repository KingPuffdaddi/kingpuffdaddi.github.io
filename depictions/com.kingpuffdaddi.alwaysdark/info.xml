<package>
	<id>com.kingpuffdaddi.alwaysdark</id>
	<name>AlwaysDark</name>
	<version>1.2.1</version>
	<compatibility>
		<firmware>
			<miniOS>13.0</miniOS>
			<maxiOS>13.5</maxiOS>
			<otherVersions>unsupported</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
